{
	"name": "68publishers/notification-bundle",
	"description": "Nette extension for user's notifications, flash messages also with Toastr (JS library) support.",
	"keywords": ["68publishers", "notification", "notification-bundle", "nette"],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Glawaty",
			"email": "tomasglawaty@icloud.com"
		}
	],
	"require": {
		"php": "^7.3 || ^8.1",
		"68publishers/smart-nette-component": "^1.0",
		"68publishers/translation-bridge": "^2.0",
		"nette/application": "^3.0.6",
		"nette/caching": "^3.0",
		"nette/di": "^3.0.3",
		"nette/http": "^3.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.0 || ^4.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"friendsofphp/php-cs-fixer": "^2.0",
		"nette/tester": "^2.2"
	},
	"autoload": {
		"psr-4": {
			"SixtyEightPublishers\\NotificationBundle\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SixtyEightPublishers\\NotificationBundle\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
